{
  "name": "victor/cloudinary-storage-bundle",
  "type": "symfony-bundle",
  "description": "file storage bundle",
  "authors": [
    {
      "name": "Victor Ilichov",
      "email": "victorlange84@gmail.com"
    }
  ],
  "autoload": {"psr-4": {"Victor\\FileStorageBundle\\": ["src"]}},
  "autoload-dev": {"psr-4": {"Victor\\FileStorageBundle\\": "tests"}},
  "config": {
    "bin-dir": "bin/"
  },
  "scripts": {
    "test": "bin/phpunit"
  },
  "require": {
    "php": ">=7.1",
    "symfony/framework-bundle": "@stable",
    "symfony/form": "@stable",
    "symfony/validator": "@stable",
    "sensio/framework-extra-bundle":"@stable",
    "symfony/twig-bridge": "@stable",
    "symfony/assetic-bundle": "@stable",
    "friendsofsymfony/jsrouting-bundle": "@stable",
    "guzzlehttp/guzzle": "^6.3"
